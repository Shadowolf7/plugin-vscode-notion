{
  "name": "plugin-vscode-notion",
  "displayName": "Notion Developer Assistant",
  "version": "1.0.0",
  "description": "Notion VS Code Plugin",
  "main": "./build/extension.js",
  "scripts": {
    "vscode:prepublish": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "watch": "tsc -watch -p tsconfig.extension.json",
    "build": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "lint": "tslint -p ./"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "notion",
          "title": "Notion",
          "icon": "assets/neurosity-logo.svg"
        }
      ]
    },
    "views": {
      "notion": [
        {
          "id": "notion",
          "name": "Device"
        }
      ]
    },
    "configuration": {
      "title": "Notion",
      "properties": {
        "notion.deviceId": {
          "type": "string",
          "default": ""
        }
      }
    }
  },
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurosity/plugin-vscode-notion.git"
  },
  "keywords": [
    "notion",
    "vscode"
  ],
  "author": "Alex Castillo and AJ Keller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neurosity/plugin-vscode-notion/issues"
  },
  "homepage": "https://github.com/neurosity/plugin-vscode-notion#readme",
  "devDependencies": {
    "@types/vscode": "^1.38.0",
    "parcel-bundler": "^1.12.4",
    "react-scripts": "^3.2.0",
    "typescript": "^3.6.3"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "dependencies": {
    "@neurosity/notion": "^3.8.0",
    "@sindresorhus/do-not-disturb": "^1.1.0",
    "@types/regression": "^2.0.0",
    "@types/universal-analytics": "^0.4.3",
    "datadog-metrics": "^0.8.1",
    "electron-google-analytics": "^0.1.0",
    "frappe-charts": "^1.3.0",
    "hot-shots": "^6.8.1",
    "osx-brightness": "^4.0.0",
    "plotly": "^1.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regression": "^2.0.1",
    "rxjs": "^6.5.3",
    "universal-analytics": "^0.4.20"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
